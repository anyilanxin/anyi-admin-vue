{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "esnext",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": ["dom", "esnext"],
    "types": ["vite/client", "@anyi/coretypes", "vue/macros-global.d.ts"]
  },
  "files.associations": {
    "*.vue": "html"
  },
  "include": ["src", "page-layout"],
  "exclude": ["**/node_modules/**", "**/dist/**"]
}
